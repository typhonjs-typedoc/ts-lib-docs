{
  "name": "@typhonjs-typedoc/ts-lib-docs",
  "version": "2023.0.0",
  "description": "Provides generated documentation for built-in Typescript lib declarations and a TypeDoc plugin to link symbols to the published docs.",
  "license": "MPL-2.0",
  "type": "commonjs",
  "author": "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)",
  "contributors": [
    "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)"
  ],
  "sideEffects": false,
  "exports": {
    "./json/dom/reflection-mapping/*": {
      "types": "./types/index-json-mapping.d.mts",
      "import": "./data/*/dom/index-mapping.mjs",
      "require": "./data/*/dom/reflection-mapping.json"
    },
    "./json/dom/reflection-names/*": {
      "types": "./types/index-json-names.d.mts",
      "import": "./data/*/dom/index-names.mjs",
      "require": "./data/*/dom/reflection-names.json"
    },
    "./json/esm/reflection-mapping/*": {
      "types": "./types/index-json-mapping.d.mts",
      "import": "./data/*/esm/index-mapping.mjs",
      "require": "./data/*/esm/reflection-mapping.json"
    },
    "./json/esm/reflection-names/*": {
      "types": "./types/index-json-names.d.mts",
      "import": "./data/*/esm/index-names.mjs",
      "require": "./data/*/esm/reflection-names.json"
    },
    "./json/worker/reflection-mapping/*": {
      "types": "./types/index-json-mapping.d.mts",
      "import": "./data/*/worker/index-mapping.mjs",
      "require": "./data/*/worker/reflection-mapping.json"
    },
    "./json/worker/reflection-names/*": {
      "types": "./types/index-json-names.d.mts",
      "import": "./data/*/worker/index-names.mjs",
      "require": "./data/*/worker/reflection-names.json"
    },
    "./typedoc/ts-links/dom/*": {
      "types": "./types/index-plugin.d.mts",
      "require": "./dist/plugin/external/typedoc/ts-links/*/dom/index.cjs"
    },
    "./typedoc/ts-links/esm/*": {
      "types": "./types/index-plugin.d.mts",
      "require": "./dist/plugin/external/typedoc/ts-links/*/esm/index.cjs"
    },
    "./typedoc/ts-links/worker/*": {
      "types": "./types/index-plugin.d.mts",
      "require": "./dist/plugin/external/typedoc/ts-links/*/worker/index.cjs"
    },
    "./typedoc/ts-links-checker": {
      "types": "./types/index-plugin.d.mts",
      "require": "./dist/plugin/external/typedoc/ts-links-checker.cjs"
    }
  },
  "devDependencies": {
    "@mdn/browser-compat-data" : "^5.2.57",
    "@rollup/plugin-node-resolve" : "^15.0.2",
    "@rollup/plugin-image": "^3.0.2",
    "@rollup/plugin-replace" : "^5.0.2",
    "@rollup/plugin-terser": "^0.4.2",
    "@types/dom-webcodecs": "^0.1.7",
    "@typhonjs-build-test/node": "^0.0.9",
    "@typhonjs-utils/file-util": "^0.0.10",
    "@typhonjs-utils/object": "^0.0.12",
    "@webgpu/types": "^0.1.32",
    "cheerio": "^1.0.0-rc.12",
    "cssnano": "^6.0.1",
    "postcss": "^8.4.23",
    "postcss-cli": "^10.1.0",
    "postcss-preset-env": "^8.3.2",
    "rollup-plugin-svelte": "^7.1.4",
    "sass": "^1.62.1",
    "svelte": "^3.59.1",
    "svelte-preprocess" : "^5.0.3",
    "ts-morph": "^18",
    "typedoc": "^0.24",
    "typedoc-plugin-extras": "^2.3.3",
    "typescript": "5.0.4"
  },
  "peerDependencies" : {
    "typedoc": ">=0.23.x"
  },
  "files": [
    "/data",
    "/dist/plugin/external",
    "/types"
  ],
  "browserslist": [">5%", "not IE 11"],
  "scripts": {
    "build-docs": "npm run build-css && cross-env CONFIG_YEAR=2023 node src/generate/generate.mjs",
    "build-css": "sass ./styles/custom.scss ./styles/custom.css && postcss ./styles/custom.css -p sass -u autoprefixer postcss-preset-env cssnano -m -o ./styles/custom.css",
    "build-plugins": "rollup -c config/rollup/rollup.config.plugins.mjs",
    "eslint": "eslint ."
  }
}
