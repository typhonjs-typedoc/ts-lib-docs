{
  "name": "@typhonjs-typescript/api-docs-libs",
  "version": "2023.0.0",
  "description": "Provides generated documentation for built-in Typescript lib declarations and a TypeDoc plugin to link symbols to the published docs.",
  "license": "MPL-2.0",
  "type": "commonjs",
  "author": "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)",
  "contributors": [
    "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)"
  ],
  "sideEffects": false,
  "exports": {
    "./json/dom/*": {
      "types": "./types/index-json.d.mts",
      "require": "./data/*/dom/url-mapping.json"
    },
    "./json/esm/*": {
      "types": "./types/index-json.d.mts",
      "require": "./data/*/esm/url-mapping.json"
    },
    "./json/worker/*": {
      "types": "./types/index-json.d.mts",
      "require": "./data/*/worker/url-mapping.json"
    },
    "./typedoc/ts-links/2023": {
      "types": "./types/index-plugin.d.mts",
      "require": "./dist/plugin/external/typedoc/ts-links/index.cjs"
    }
  },
  "devDependencies": {
    "@mdn/browser-compat-data" : "^5.2.57",
    "@types/dom-webcodecs": "^0.1.7",
    "@typhonjs-build-test/node": "^0.0.9",
    "@typhonjs-utils/file-util": "^0.0.10",
    "@typhonjs-utils/object": "^0.0.12",
    "@webgpu/types": "^0.1.32",
    "ts-morph": "^18",
    "typedoc": "^0.24",
    "typedoc-plugin-extras": "^2.3.3",
    "typescript": "5.0.4"
  },
  "peerDependencies" : {
    "typedoc": ">=0.23.x"
  },
  "files": [
    "/dist",
    "/types"
  ],
  "browserslist": [">5%", "not IE 11"],
  "scripts": {
    "build-docs": "cross-env CONFIG_YEAR=2023 node src/generate/generate.mjs",
    "build-plugins": "rollup -c",
    "eslint": "eslint ."
  }
}
